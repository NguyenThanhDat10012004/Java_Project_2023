<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" >
   <children>
      <VBox alignment="TOP_CENTER" layoutX="184.0" layoutY="52.0" prefHeight="600.0" prefWidth="1000.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label alignment="CENTER" contentDisplay="TOP" prefHeight="88.0" prefWidth="419.0" text="Conglatuation !" textAlignment="CENTER">
               <font>
                  <Font name="Arial" size="19.0" />
               </font>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </Label>
            <HBox>
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="143.0" prefWidth="665.0" text=" Score:" textAlignment="CENTER" />
                  <Label fx:id="scoreLabel" prefHeight="142.0" prefWidth="490.0" text="Label" />
               </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="128.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="143.0" prefWidth="659.0" text=" Best Score:" textAlignment="CENTER" />
                  <Label fx:id="bestScoreLabel" prefHeight="142.0" prefWidth="490.0" text="Label" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="123.0" prefWidth="1000.0" spacing="150.0">
               <children>
                  <Button fx:id="playAgainButton" mnemonicParsing="false" text="Play Again" />
                  <Button fx:id="endGameButton" mnemonicParsing="false" onAction="#endGame" text="END" />
               </children>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
